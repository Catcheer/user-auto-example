extends ./layout.jade
block content
  form(action="/signup",method="POST")
    fieldset
      legend Sign up
      p
        label First
        input(name="first",type="text")
      p
        label last
        input(name="last",type="text")
      p
        label Email
        input(name="email",type="text")
      p
        label password
        input(name="password",type="password")
      p
        button Submit
      p
        a(href="/") Go Back